{
  "name": "@macrostrat/ui-components",
  "version": "0.5.2",
  "description": "UI components for React and blueprint.js",
  "repository": "https://github.com/UW-Macrostrat/ui-components.git",
  "style": "lib/esm/index.css",
  "module": "lib/esm",
  "main": "lib/cjs",
  "typings": "lib/types",
  "scripts": {
    "prepublish": "npm run build",
    "watch:main": "rollup -c -w",
    "watch:types": "tsc --emitDeclarationOnly -w",
    "dev": "npm-run-all -p --aggregate-output watch:main watch:types",
    "build:types": "tsc --emitDeclarationOnly; exit 0",
    "build": "rollup -c && npm run build:types",
    "deploy-docs": "scripts/deploy-docs"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@blueprintjs/core": "^3.31.0",
    "@blueprintjs/datetime": "^3.18.6",
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@macrostrat/hyper": "^1.2.10",
    "@types/classnames": "^2.2.9",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.8",
    "@types/react-hyperscript": "^3.0.4",
    "@types/react-router-dom": "^5.1.5",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "core-js": "^3.6.4",
    "date-fns": "^2.15.0",
    "fp-ts": "^2.5.3",
    "immutability-helper": "^3.1.1",
    "io-ts": "^2.1.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.5",
    "react-dropzone": "^11.0.3",
    "react-hyperscript": "^3.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-json-view": "^1.19.1",
    "react-virtualized": "^9.21.2",
    "react-window": "^1.8.5",
    "underscore": "^1.9.1",
    "use-async-effect": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "babel-plugin-emotion": "^10.0.14",
    "docz": "^2.2.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^3.2.0",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.27.1",
    "rollup-plugin-postcss": "^3.1.8",
    "stylus": "^0.54.7",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
