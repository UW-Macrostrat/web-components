{
  "name": "@macrostrat/web-components",
  "version": "1.0.0",
  "description": "Web components for stratigraphy and geologic mapping managed by Macrostrat",
  "private": true,
  "scripts": {
    "dev:legacy": "npm-run-all -l -p dev watch:**",
    "watch:map": "npm --prefix packages/map-components run watch:main",
    "watch:ui": "npm --prefix packages/ui-components run dev",
    "watch:column": "npm --prefix packages/column-components run watch",
    "watch:timescale": "npm --prefix packages/timescale run watch",
    "build": "parcel build --public-url . apps/index.html",
    "deploy": "./deploy/deploy-steno.sh",
    "clean": "rm -rf .cache dist",
    "dev": "concurrently \"yarn dev:storybook\" \"yarn dev:concepts\"",
    "dev:storybook": "yarn workspace storybook run dev",
    "dev:concepts": "yarn workspace concept-apps run dev",
    "build-storybook": "build-storybook",
    "publish-libs": "node publish.mjs"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "alias": {
    "@macrostrat/column-components": "@macrostrat/column-components/src",
    "@macrostrat/map-components": "@macrostrat/map-components/src",
    "@macrostrat/ui-components": "./packages/ui-components/src",
    "@macrostrat/api-views": "./packages/api-views/src",
    "common": "./packages/common/src"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@parcel/resolver-glob": "^2.0.0-nightly.2338",
    "@parcel/transformer-postcss": "^2.0.0-nightly.778",
    "@types/d3-geo": "^2.0.0",
    "@types/geojson": "^7946.0.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.2.2",
    "css-loader": "^3.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.5",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "style-loader": "^1.3.0",
    "stylus": "^0.54.7",
    "stylus-loader": "^4.3.3"
  },
  "workspaces": [
    "packages/*",
    "concept-apps",
    "storybook"
  ],
  "dependencies": {
    "@macrostrat/api-views": "file:./packages/api-views",
    "@macrostrat/column-components": "file:./packages/column-components",
    "@macrostrat/hyper": "^2.0.0",
    "@macrostrat/map-components": "file:./packages/map-components",
    "@macrostrat/timescale": "file:./packages/timescale",
    "@macrostrat/ui-components": "file:./packages/ui-components",
    "@react-hook/size": "^2.1.1",
    "@types/react": "^16.14.2",
    "@vx/axis": "^0.0.199",
    "@vx/gradient": "^0.0.199",
    "@vx/scale": "^0.0.199",
    "@vx/shape": "^0.0.199",
    "axios": "^0.21.1",
    "chalk": "^5.0.1",
    "chroma-js": "^2.1.0",
    "core-js": "^3.6.1",
    "d3": "^5.15.0",
    "d3-array": "^2.8.0",
    "d3-geo": "^3.0.1",
    "d3-geo-voronoi": "^1.6.0",
    "d3-path": "^1.0.9",
    "d3-scale-chromatic": "^1.5.0",
    "d3-selection": "^1.4.1",
    "geologic-patterns": "github:davenquinn/geologic-patterns",
    "immutability-helper": "^3.0.1",
    "labella": "^1.1.4",
    "postcss": "^8.3.11",
    "prop-types": "^15.7.2",
    "react-draggable": "^4.2.0",
    "react-images": "^1.1.0-beta.0",
    "react-json-view": "^1.19.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.15",
    "react-spring": "^8.0.27",
    "react-svg-textures": "^1.4.7",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.50.0",
    "sass-loader": "^10.2.1",
    "supercluster": "^7.1.3",
    "topojson-client": "^3.1.0",
    "ui-box": "^3.0.2",
    "underscore": "^1.12.0",
    "use-async-effect": "^2.2.1"
  },
  "prettier": {}
}
