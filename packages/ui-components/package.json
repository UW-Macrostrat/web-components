{
  "name": "@macrostrat/ui-components",
  "version": "1.0.4",
  "description": "UI components for React and blueprint.js",
  "repository": "https://github.com/UW-Macrostrat/ui-components.git",
  "style": "lib/esm/index.css",
  "module": "lib/esm",
  "main": "lib/cjs",
  "typings": "lib/types",
  "scripts": {
    "prepack": "npm run build",
    "watch:main": "rollup -c -w",
    "watch:types": "tsc --emitDeclarationOnly -w",
    "dev": "npm-run-all -p --aggregate-output watch:main watch:types",
    "build:types": "tsc --emitDeclarationOnly",
    "build": "rollup -c && npm run build:types",
    "deploy-docs": "scripts/deploy-docs"
  },
  "author": "Daven Quinn",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@blueprintjs/core": "^3.31.0",
    "@blueprintjs/datetime": "^3.18.6",
    "@blueprintjs/popover2": "^0.13.0",
    "@macrostrat/hyper": "^1.4.0",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "axios": "^0.21.4",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.0",
    "core-js": "^3.6.4",
    "d3-array": "^3.0.4",
    "date-fns": "^2.15.0",
    "fp-ts": "^2.5.3",
    "immutability-helper": "^3.1.1",
    "io-ts": "^2.1.2",
    "query-string": "^7.0.0",
    "react-dropzone": "^11.4.2",
    "react-intersection-observer": "^8.32.1",
    "react-json-tree": "^0.15.0",
    "underscore": "^1.9.1",
    "use-async-effect": "^2.2.1",
    "use-element-dimensions": "^2.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^3.2.0",
    "prettier": "^2.4.1",
    "pretty-quick": "^2.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.38.1",
    "rollup-plugin-postcss": "^3.1.8",
    "stylus": "^0.54.8",
    "typescript": "^4"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17",
    "react-dom": "^16.8.6 || ^17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
