{
  "name": "@macrostrat/ui-components",
  "version": "4.2.0",
  "description": "UI components for React and Blueprint.js",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "node": "dist/node/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts",
      "node": "./dist/node/index.js"
    }
  },
  "targets": {
    "node": {
      "engines": {
        "node": ">=14"
      }
    }
  },
  "scripts": {
    "dev": "parcel watch",
    "build": "rm -rf dist && parcel build"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "Daven Quinn",
  "contributors": [
    {
      "name": "Casey Idzikowski"
    }
  ],
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.10.2",
    "@blueprintjs/datetime2": "^2.3.11",
    "@macrostrat/hyper": "^3.0.6",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18",
    "axios": "^1.7.9",
    "chroma-js": "^2.4.2||^3.0.0",
    "classnames": "^2.3.0",
    "d3-array": "^3.2.4",
    "immutability-helper": "^3.1.1",
    "query-string": "^9.0.0",
    "react-color": "^2.19.3",
    "react-dropzone": "^11.4.2",
    "react-intersection-observer": "^8.32.1",
    "react-json-tree": "^0.19.0",
    "ui-box": "^5.4.1",
    "underscore": "^1.13.1",
    "use-async-effect": "^2.2.1",
    "use-element-dimensions": "^2.1.3",
    "zustand": "^5.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17 || ^18 || ^19",
    "react-dom": "^16.8.6 || ^17 || ^18 || ^19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UW-Macrostrat/web-components.git",
    "directory": "packages/ui-components"
  },
  "devDependencies": {
    "parcel": "^2.15.4"
  }
}
