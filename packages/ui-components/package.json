    "@macrostrat/hyper": "^2.0.1",
    "@mantine/core": "^4.1.3",
    "@mantine/hooks": "^4.1.3",
    "axios": "^0.21.4",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.0",
