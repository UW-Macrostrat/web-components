    "@types/geojson": "^7946.0.16",
    "@types/react-dom": "^18",
    "axios": "^1.13.2",
    "d3-array": "^3.2.4",
    "d3-drag": "^3.0.0",
    "d3-geo": "^3.1.1",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "kld-intersections": "^0.7.0",
    "kld-path-parser": "^0.2.1",
    "topojson-client": "^3.1.0"
