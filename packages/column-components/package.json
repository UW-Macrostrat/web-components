      "style": "./dist/column-components.css"
    },
    "./package.json": "./package.json",
    "./style.css": "./dist/column-components.css",
    "./dist/column-components.css": "./dist/column-components.css"
  },
  "scripts": {
    "build": "bundle-library ."
  },
