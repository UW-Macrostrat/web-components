  "author": "Daven Quinn <dev@davenquinn.com>",
  "license": "MIT",
  "devDependencies": {
    "@macrostrat/web-components-bundler": "workspace:^"
  },
  "dependencies": {
    "@blueprintjs/core": "^6.6.1",
    "@macrostrat/hyper": "^3.0.6",
    "@macrostrat/stratigraphy-utils": "workspace:^",
