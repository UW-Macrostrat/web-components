    "d3-format": "^3.1.2",
    "d3-path": "^3.1.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "immutability-helper": "^3.1.1",
    "labella": "^1.1.4",
    "react-draggable": "^4.4.5",
    "react-scroll": "^1.7.16",
    "react-select": "^3.0.8",
    "react-svg-textures": "^1.4.7",
    "ui-box": "^5.4.1",
    "uuid": "^7.0.3"
