{
  "name": "@macrostrat/column-components",
  "version": "0.8.2",
  "description": "React rendering primitives for stratigraphic columns",
  "keywords": [
    "geology",
    "react",
    "vector-graphics",
    "data-visualization"
  ],
  "main": "dist/cjs",
  "module": "dist/esm",
  "style": "dist/esm/index.css",
  "typings": "dist/types",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:main": "rollup -c rollup.config.ts -w",
    "watch:types": "tsc --emitDeclarationOnly -w",
    "watch": "npm-run-all -p watch:main watch:types",
    "dev": "npm run prebuild && npm run watch",
    "build:main": "rollup -c rollup.config.ts",
    "build:types": "tsc --emitDeclarationOnly; exit 0",
    "build": "npm-run-all -s prebuild build:types build:main",
    "start": "npm run dev",
    "prebuild": "rimraf dist"
  },
  "author": "Daven Quinn <dev@davenquinn.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-typescript": "^7.11.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^3.2.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.38.1",
    "rollup-plugin-postcss": "^3.1.8",
    "stylus": "^0.54.7",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@macrostrat/hyper": "^1.2.0",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "d3-axis": "^1.0.12",
    "d3-format": "^1.4.3",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "fscreen": "^1.0.2",
    "immutability-helper": "^3.0.2",
    "labella": "^1.1.4",
    "prop-types": "^15.7.2",
    "react-color": "^2.18.0",
    "react-draggable": "^4.2.0",
    "react-hyperscript": "^3.2.0",
    "react-images": "^1.1.0-beta.1",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.16",
    "react-select": "^3.0.8",
    "react-svg-textures": "^1.4.7",
    "ui-box": "^3.0.2",
    "uuid": "^7.0.3"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.22.3",
    "@macrostrat/ui-components": "^0.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "prettier": {}
}
