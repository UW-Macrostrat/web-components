{
  "name": "@macrostrat/column-components",
  "version": "0.0.1",
  "description": "React rendering primitives for stratigraphic columns",
  "keywords": [
    "geology",
    "react",
    "vector-graphics",
    "data-visualization"
  ],
  "main": "dist/cjs",
  "module": "dist/esm",
  "style": "dist/column-components.css",
  "typings": "dist/types",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:main": "rollup -c rollup.config.ts -w",
    "watch:types": "tsc --emitDeclarationOnly -w 2&> /dev/null",
    "watch": "npm-run-all --aggregate-output --parallel watch:**",
    "dev": "npm run prebuild && npm run watch",
    "start": "npm run dev",
    "prebuild": "rimraf dist"
  },
  "author": "Daven Quinn <dev@davenquinn.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.7",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-modules": "^1.4.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-coffee-script": "^2.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "rollup-plugin-multi-entry": "^2.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-sourcemaps": "^0.5.0",
    "tsc-silent": "^1.2.1",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "@macrostrat/hyper": "^1.2.0",
    "autobind": "^1.0.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "d3": "^5.15.0",
    "d3-format": "^1.4.3",
    "d3-path": "^1.0.9",
    "d3-scale": "^3.2.1",
    "d3-selection": "^1.4.1",
    "fscreen": "^1.0.2",
    "labella": "^1.1.4",
    "prop-types": "^15.7.2",
    "react-color": "^2.18.0",
    "react-draggable": "^4.2.0",
    "react-hyperscript": "^3.2.0",
    "react-images": "^1.1.0-beta.1",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "react-svg-textures": "^1.4.7",
    "ui-box": "^3.0.2",
    "underscore": "^1.9.2",
    "uuid": "^3.3.3"
  },
  "peerDependencies": {
    "@blueprintjs/core": "^3.22.3",
    "@macrostrat/ui-components": "^0.1.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
