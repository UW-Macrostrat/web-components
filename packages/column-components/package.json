{
  "name": "@macrostrat/column-components",
  "version": "1.0.3",
  "description": "React rendering primitives for stratigraphic columns",
  "keywords": [
    "geology",
    "react",
    "vector-graphics",
    "data-visualization"
  ],
  "main": "./dist/esm/index.js",
  "type": "module",
  "source": "./src/index.ts",
  "types": "./dist/esm/index.d.ts",
  "node": "./dist/node/index.js",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts",
      "node": "./dist/node/index.js"
    },
    "./stories/base-section": {
      "source": "./stories/base-section.ts"
    }
  },
  "targets": {
    "node": {
      "engines": {
        "node": ">=14"
      }
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "dev": "parcel watch",
    "build": "rm -rf dist && parcel build",
    "check": "tsc --noEmit"
  },
  "author": "Daven Quinn <dev@davenquinn.com>",
  "license": "MIT",
  "devDependencies": {
    "parcel": "^2.13.3",
    "stylus": "^0.59.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@blueprintjs/core": "^5.0.0",
    "@macrostrat/hyper": "^3.0.6",
    "@macrostrat/timescale": "workspace:^",
    "@macrostrat/ui-components": "workspace:^",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "d3-axis": "^3.0.0",
    "d3-format": "^3.1.0",
    "d3-path": "^3.1.0",
    "d3-scale": "^3.2.1",
    "d3-selection": "^3.0.0",
    "fscreen": "^1.0.2",
    "immutability-helper": "^3.0.2",
    "labella": "^1.1.4",
    "react-color": "^2.18.0",
    "react-draggable": "^4.4.5",
    "react-images": "^1.1.0-beta.1",
    "react-router-dom": "^5.1.2",
    "react-scroll": "^1.7.16",
    "react-select": "^3.0.8",
    "react-svg-textures": "^1.4.7",
    "ui-box": "^5.4.1",
    "uuid": "^7.0.3"
  },
  "peerDependencies": {
    "react": "^16.12.0||^17.0.0||^18",
    "react-dom": "^16.12.0||^17.0.0||^18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UW-Macrostrat/web-components.git",
    "directory": "packages/column-components"
  }
}
