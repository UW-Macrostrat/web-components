  "module": "dist/index.js",
  "types": "dist/types.d.ts",
  "style": "dist/index.css",
  "scripts": {
    "dev": "parcel watch",
    "build": "parcel build"
  },
  "authors": [
    "Casey Idzikowski",
    "Daven Quinn"
  ],
  "license": "MIT",
  "devDependencies": {
    "@blueprintjs/core": "^5.10.2",
